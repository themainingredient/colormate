{
  "name": "colormate",
  "version": "1.4.26",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/themainingredient/colormate.git"
  },
  "description": "Colormate is a kickass sketch plugin that will help you figure out how in the hell you ended up with 457 different greys, instead of the 1 grey Mandy gave you in the handover",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Colormate",
    "manifest": "src/manifest.json",
    "identifier": "com.colormate.plugin",
    "main": "colormate.sketchplugin",
    "assets": [
      "assets/**/*",
      "resources/app/assets/**/*"
    ]
  },
  "scripts": {
    "publish": "skpm publish",
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch",
    "postinstall": "npm run build && skpm-link",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "./node_modules/.bin/eslint src/ resources/ --ext .jsx,.js --max-warnings 0",
    "log": "skpm log -f",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@semantic-release/exec": "^3.3.3",
    "@semantic-release/github": "^5.3.2",
    "@skpm/builder": "^0.5.11",
    "@skpm/extract-loader": "^2.0.2",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^1.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv": "^7.0.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.1",
    "jest": "^24.5.0",
    "react-hooks-testing-library": "^0.4.0",
    "react-svg-loader": "^2.1.0",
    "react-testing-library": "^6.0.4",
    "semantic-release": "^15.13.12"
  },
  "resources": [
    "resources/**/*.js"
  ],
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "sketch": "^0.2.0",
    "sketch-module-web-view": "^2.1.5",
    "styled-components": "^4.2.0"
  },
  "author": "The Main Ingredient",
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
