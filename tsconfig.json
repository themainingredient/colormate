{
    "compilerOptions": {
      "target": "es5",
      "module": "esnext",
      "downlevelIteration": true,
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true
    },
    "include": [
        "./src/**/*",
        "./resources/**/*",
      ],
  }